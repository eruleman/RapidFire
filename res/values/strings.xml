    <!-- The question and answer bank now follow: -->
    <!-- Some formatting notes: &lt;br/> is a line break. -->

    <string name="question0">Factor the following:</string>
    <string name="question0_math_expression">x^{2} - 64</string>
    <string name="question0_correct_answer">(x - 8)(x + 8)</string>
    <string name="question0_answer_explanation">This is a difference of two squares, x&lt;sup>&lt;small>2&lt;/small>&lt;/sup> and 8&lt;sup>&lt;small>2&lt;/small>&lt;/sup>."</string>
    <string name="question0_answer0">(x - 8)(x + 8)</string>
    <string name="question0_answer1">(x - 8)(x - 8)</string>
    <string name="question0_answer2">(x + 8)(x + 8)</string>
    <string name="question0_answer3">(8 + x)(8 + x)</string>
    <!-- x^2, using &lt; in place of "<" in order to escape -->
    <string name="question0_answer4">(8 - x&lt;sup>&lt;small>2&lt;/small>&lt;/sup>)(8 - x)</string>

    <!-- Topic: Addition and Subtraction of Algebraic Expressions -->
    <string name="question1">Collect like terms and simplify the following:</string>
    <string name="question1_math_expression">3x^{2} + 2x - x^{2} - 4x</string>
    <string name="question1_correct_answer">2x&lt;sup>&lt;small>2&lt;/small>&lt;/sup> - 2x</string>
    <string name="question1_answer_explanation">Question 1 Explanation</string>
    <string name="question1_answer0">4x&lt;sup>&lt;small>2&lt;/small>&lt;/sup> + 2x</string>
    <string name="question1_answer1">2x&lt;sup>&lt;small>2&lt;/small>&lt;/sup> + 2x</string>
    <string name="question1_answer2">4x&lt;sup>&lt;small>2&lt;/small>&lt;/sup> - 4x</string>
    <string name="question1_answer3">2x&lt;sup>&lt;small>2&lt;/small>&lt;/sup> - 2x</string>
    <string name="question1_answer4">2x - 2x&lt;sup>&lt;small>2&lt;/small>&lt;/sup></string>
